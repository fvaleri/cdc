{
    "name": "xml-jms-sink",
    "config": {
        "connector.class": "io.confluent.connect.jms.JmsSinkConnector",
        "tasks.max": "1",
        "topics": "localhost.cdc.customers",
        "java.naming.factory.initial": "org.apache.activemq.jndi.ActiveMQInitialContextFactory",
        "java.naming.provider.url": "tcp://localhost:61616",
        "java.naming.security.principal": "admin",
        "java.naming.security.credentials": "admin",
        "jndi.connection.factory": "connectionFactory",
        "jms.destination.type": "queue",
        "jms.destination.name": "CustomersXML",
        "confluent.topic.bootstrap.servers": "localhost:9090",
        "confluent.topic.replication.factor": "1",
        "transforms": "XmlWriter",
        "transforms.XmlWriter.type": "it.fvaleri.cdc.XmlWriter"
    }
}
